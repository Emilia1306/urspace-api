{
    "compilerOptions": {
      "moduleResolution": "node",
      "target": "es6",
      "module": "commonjs",
      "rootDir": "src",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "typeRoots": ["./node_modules/@types", "./types"]
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  